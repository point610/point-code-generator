{
  "name": "base-code-generator-nkw",
  "description": "牛客网java代码模板生成器",
  "basePackage": "com.point",
  "version": "1.0",
  "author": "point",
  "createTime": "2024-01-20",
  "fileConfig": {
    "inputRootPath": "E:/Java_Projection/point-code-generator/demo-project/nkw-demo",
    "outputRootPath": ".temp",
    "type": "dir",
    "files": [
      {
        "inputPath": "src/main/java/com/point/Main.java.ftl",
        "outputPath": "src/com/yupi/acm/MainTemplate.java",
        "type": "file",
        "generateType": "dynamic"
      },
      {
        "inputPath": ".gitignore",
        "outputPath": ".gitignore",
        "type": "file",
        "generateType": "static"
      },
      {
        "inputPath": "README.md",
        "outputPath": "README.md",
        "type": "file",
        "generateType": "static"
      }
    ]
  },
  "modelConfig": {
    "models": [
      {
        "fieldName": "loop",
        "type": "boolean",
        "description": "是否生成循环",
        "defaultValue": false,
        "abbr": "l"
      },
      {
        "fieldName": "author",
        "type": "String",
        "description": "作者注释",
        "defaultValue": "point",
        "abbr": "a"
      },
      {
        "fieldName": "printList",
        "type": "boolean",
        "description": "是否输出数组信息",
        "defaultValue": true,
        "abbr": "pl"
      }
    ]
  }
}